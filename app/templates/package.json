  "repository": {
    "type": "git",
    "url": "git://github.com/<%= userName %>/<%= appName %>.git"
  },
  "author": "<%= userName %> <<%= userEmail %>>",
  "license": "<%= license %>",
  "dependencies": {
    "chalk": "^1.0.0",
    "lodash": "^4.13.1",
    "yeoman-character": "^1.1.0",
    "yeoman-generator": "^0.24.0",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "yeoman-test": "^1.0.0",
    "yeoman-assert": "^2.0.0",
    "eslint": "^2.1.0",
    "eslint-config-xo-space": "^0.13.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^2.0.0",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-istanbul": "^0.10.3",
